{
  "name": "@jbernavaprah/jovo-vonage-platform",
  "version": "0.0.1",
  "description": "Vonage Integration for Jovo",
  "main": "dist/cjs/index.js",
  "module": "dist/esm5/index.js",
  "es2015": "dist/esm2015/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "sample-requests"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -b tsconfig.build.cjs.json tsconfig.build.esm5.json tsconfig.build.esm2015.json tsconfig.build.types.json",
    "watch": "tsc -b tsconfig.build.cjs.json tsconfig.build.esm5.json tsconfig.build.esm2015.json tsconfig.build.types.json --watch",
    "format": "prettier -w -l src test package.json",
    "eslint": "eslint src test --fix --ext .ts",
    "rimraf": "rimraf dist",
    "preversion": "npm run eslint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jBernavaPrah/jovo-vonage-platform.git"
  },
  "keywords": [
    "Jovo",
    "Vonage",
    "Platform"
  ],
  "author": "Jure Bernava Prah <webjure@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jBernavaPrah/jovo-vonage-platform/issues"
  },
  "homepage": "https://github.com/jBernavaPrah/jovo-vonage-platform#readme",
  "dependencies": {
    "@jovotech/cli-core": "^4.1.9",
    "@jovotech/model": "^4.0.0",
    "class-transformer": "^0.5.1",
    "lodash.clonedeep": "^4.5.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jovotech/common": "^4.5.6",
    "@jovotech/filebuilder": "^0.0.1",
    "@jovotech/framework": "^4.5.6",
    "@jovotech/output": "^4.5.6",
    "@types/jest": "^26.0.20",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^12.20.37",
    "@types/tv4": "^1.2.29",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "global-dirs": "^3.0.0",
    "global-modules": "^2.0.0",
    "jest": "^29.2.1",
    "nock": "^13.0.11",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "symlink-dir": "^4.1.0",
    "ts-jest": "^29.0.4",
    "typescript": "~4.4.4"
  },
  "peerDependencies": {
    "@jovotech/common": "^4.2.16",
    "@jovotech/filebuilder": "^0.0.1",
    "@jovotech/framework": "4.5.6",
    "@jovotech/output": "4.5.6"
  },
  "prettier": ".prettierrc.js",
  "publishConfig": {
    "access": "public"
  }
}
